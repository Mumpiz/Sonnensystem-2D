


Type TSolarPlanetParameter
	Global X:Double
	Global Y:Double
	
	Field SunNormalMass:Double = 1.0
	Field SunRealMass:Double = Float(1.991)*10^Int(30)
	
	Field EarthNormalMass:Double = 5.979*10^24 / SunRealMass
	Field EarthRealMass:Double = 5.979*10^24
	
	Field MoonNormalMass:Double = 7.349*10^22 / SunRealMass
	Field MoonRealMass:Double = 7.349*10^22
	
	Field JupiterNormalMass:Double = 1.901*10^27 / SunRealMass
	Field JupiterRealMass:Double = 1.901*10^27
End Type




